<h1>Transactions</h1>

<table>
  <thead>
    <tr>
      <th><u>Payer</u></th>
      <th><u>Points</u></th>
      <th><u>Time</u></th>
      <th colspan="3"></th>
      <th rowspan="6"></th>
    </tr>
  </thead>

  <tbody>
    <% @transactions.order('updated_at DESC').each do |t| %>
        <tr>
        <td><%= t.payer %></td>
        <td><%= t.points %></td>
        <td><%= t.created_at %></td>
   
        </tr>
        <% end %>       
    </tbody>
</table>


